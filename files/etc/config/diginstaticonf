config globals 'globals'
	list protocol 'EMPTY.пусто'
	list protocol 'SNMP.датчик устройства NetPing по SNMP'
	list protocol 'GPIO.датчик устройства NetPing по GPIO'
	list status '0.Normal'
	list status '1.Timeout'
	list status '2.Error'
	option maxsensors 32
	option max_gpio_sensors 4
	option default_protocol 'GPIO' # from list protocol 

config sensor 'sensor_prototype_empty'
	option name 'SensorName'
	option description 'Description'
	option ton_desc 'ON'
	option toff_desc 'OFF'
	option protocol 'empty'

config sensor 'sensor_prototype_snmp'
	option name 'SensorName'
	option description 'Description'
	option ton_desc 'ON'
	option toff_desc 'OFF'
	option protocol 'snmp'
	option snmp_addr '127.0.0.1'
	option snmp_port '161'
	option community '0'
	option oid '0'
	option timeout '1'
	option period '3'

config sensor 'sensor_prototype_gpio'
	option name 'SensorName'
	option description 'Description'
	option ton_desc 'ON'
	option toff_desc 'OFF'
	option protocol 'gpio'
	option countable '0'
	option min_pulse_time '100'
	option debounce_time '100'
	option count_type '0/1/0'
