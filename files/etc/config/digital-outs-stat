
config relay 'globals'
	list protocol 'snmp.реле устройства NetPing по SNMP'
	list protocol 'gpio.реле устройства NetPing по GPIO'

config relay 'max_relays'
	option max_snmp_relays '64'
	option max_gpio_relays '6'

config relay 'relay_prototype_snmp'
	option name 'Name Relay'
	option memo 'Relay SNMP'
	option start_state 'NO'
	option state_alias_0 'Выключено'
	option state_alias_1 'Включено'
	option protocol 'snmp'
	option community_rd '0'
	option community_wr '0'
	option address '0'
	option port '0'
	option oid_rd '0'
	option oid_wr '0'
	option type_oid '0'
	option timeout '5'
	option period '1'

config relay 'relay_prototype_gpio'
	option name 'Name Relay'
	option memo 'Relay GPIO'
	option start_state 'NO'
	option state_alias_0 'Выключено'
	option state_alias_1 'Включено'
	option protocol 'gpio'
	option period '1'
