config globals 'globals'
	list event 'temperature'
	list event 'userlogin'
	list event 'userlogout'
	list event 'configchanged'
	list event 'operation'
	list event 'error'
	list event 'statechanged'
	list event 'humidity'
	list method 'email'
	list method 'syslog'
	list method 'snmptrap'
	list method 'sms'
	list method 'webhook'

config notify 'prototype'
	option name 'Notify'
	option state '0'
	option event 'statechanged'
	option expression '-'
	option method 'email'
	option text 'Message text here'
	option sendto 'addr1'
	list timetable 'Пн,Вт,Ср,Чт,Пт,Сб,Вс 00:00:00-23:59:59'


