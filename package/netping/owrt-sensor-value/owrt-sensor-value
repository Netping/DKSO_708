
config sensor 'globals'
	option default_memo 'Sensor'
	list status '0.Норма'
	list status '1.Таймаут'
	list status '2.Ошибка'
	option default_timeout '5'
	option default_period '1'

config sensor 'sensor_prototype_snmp'
	option type_struct 'sensor'
	option memo 'Sensor SNMP'
	option proto 'SNMP'
	option community '0'
	option address '0'
	option port '0'
	option timeout '6'

config sensor 'resource_prototype_snmp'
	option type_struct 'resource'
	option unit 'Z'
	option precision '0'
	option oid '0'
	option type_oid '0'
	option type_resource '0'
	option max_hysteresis '0'
	option min_hysteresis '0'
	option period '1'

config sensor 'sensor_prototype_1wire'
	option type_struct 'sensor'
	option memo 'Sensor 1wire'
	option proto '1wire'
